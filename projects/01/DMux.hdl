/**
 * Demultiplexor:
 * {a, b} = {in, 0} if sel == 0
 *          {0, in} if sel == 1
 */

CHIP DMux {
    IN in, sel;
    OUT a, b;

    PARTS:
    Not (in=sel, out=notsel);
    Nand( a=in, b=notsel, out=x);
    Nand( a=sel, b=in, out=y);
    Nand( a=x, b=x, out=a);
    Nand( a=y, b=y, out=b);

}
